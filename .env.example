# PipeX Environment Variables Configuration
# Copy this file to .env and fill in your actual values

# AWS Configuration for S3 operations
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1
BUCKET_NAME=your-s3-bucket-name

# API Configuration
API_TOKEN=your-api-bearer-token
API_ENDPOINT=https://api.example.com/data

# Database Configuration (MySQL)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=your-mysql-username
MYSQL_PASSWORD=your-mysql-password
MYSQL_DATABASE=your-database-name

# Database Configuration (PostgreSQL)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=your-postgres-username
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_DATABASE=your-database-name

# MongoDB Configuration
MONGO_HOST=localhost
MONGO_PORT=27017
MONGO_USER=your-mongo-username
MONGO_PASSWORD=your-mongo-password
MONGO_DATABASE=your-mongo-database

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/pipex.log

# Pipeline Configuration
PIPELINE_NAME=my-etl-pipeline
ENVIRONMENT=development

# Optional: Custom timestamp format for file naming
TIMESTAMP=$(date +%Y%m%d_%H%M%S)

# Security Note:
# Never commit the actual .env file to version control
# This .env.example file should contain only placeholder values